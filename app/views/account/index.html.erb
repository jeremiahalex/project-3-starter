<div class="container">
  <div class="row">
    <div class="col s12 m10 offset-m1">
      <h4>Support Tickets</h4>
      <table>
        <thead>
          <tr>
            <th>Product Name</th>
            <th>Date Created</th>
            <th>Issue</th>
            <th>Status</th>
            <th>View</th>
          </tr>
        </thead>

        <tbody>
          <%= render partial: 'open_ticket', collection: @open_tickets %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="row">
    <div class="col s12 m10 offset-m1">
      <h4>Registered Products</h4>
      <table>
        <thead>
          <tr>
            <th>Product Name</th>
            <th>Serial Number</th>
            <th style="text-align: center;">Warranty</th>
            <th style="text-align: center;">Start Date</th>
            <th style="text-align: center;">End Date</th>
            <th style="text-align: center;">Support Ticket</th>
          </tr>
        </thead>

        <tbody>
            <%= render partial: 'listing', collection: @listings %>
        </tbody>
      </table>
    </div>
  </div>
</div>
