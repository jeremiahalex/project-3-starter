<div class="container">
  <div class="row">
    <div class="col s12 m10 offset-m1">
      <h4>Support Tickets</h4>
      <table>
        <thead>
          <tr>
            <th>Product</th>
            <th>Ticket Status</th>
            <th>Last Response</th>
          </tr>
        </thead>

        <tbody>
          <td>Dyson V8 Absolute+</td>
          <td>Live</td>
          <td>22/01/2023</td>
        </tbody>
      </table>
    </div>
  </div>
  <div class="row">
    <div class="col s12 m10 offset-m1">
      <h4>Registered Products</h4>
      <table>
        <thead>
          <tr>
            <th>Product Name</th>
            <th>Serial Number</th>
            <th>Warranty Status</th>
            <th>Warranty Start Date</th>
            <th>Warranty End Date</th>
          </tr>
        </thead>

        <tbody>
          <tr>
            <%= render partial: 'listing', collection: @listings %>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
