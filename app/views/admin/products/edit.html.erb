<%= render "admin/admin_nav" %>
<div class="container">
  <div class="row">
    <div class="input-field col s12 m10 offset-m1">
      <h4>Edit Product</h4>
    </div>
  </div>

  <%= render "product", product: @product, url: admin_product_path, html: { method: "patch"} %>

  <div class="row">
    <div class="col s12 m10 offset-m1">
      <%= link_to fa_icon("arrow-left ", text: "Back to Product Listing"), admin_products_path, class:"btn waves-effect waves-light light-blue darken-3" %>
    </div>
  </div>
</div>
