<div class="container">
  <div class="section">
    <div class="row">
      <div class="col s12 m10 offset-m1">
        <h3>Products</h3>
        <table>
          <thead>
            <tr>
              <th>Product Name</th>
              <th>Model Number</th>
              <th>Serial Number</th>
              <th>Warranty</th>
              <th>Edit</th>
              <th>View</th>
            </tr>
          </thead>

          <tbody>
            <%= render partial: 'listing', collection: @listings %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
